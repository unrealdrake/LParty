{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015", "dom", "es2015.promise"]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}